        <Version>0.0.1</Version>

        <RootNamespace>Phrasefable.StardewMods.AggressiveAcorns.InGameTest</RootNamespace>
        <TargetFramework>net452</TargetFramework>

        <EnableModZip>False</EnableModZip>
